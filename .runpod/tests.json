{
  "tests": [
    {
      "name": "illustrious_xl_txt2img",
      "input": {
        "workflow": {
          "1": {
            "inputs": {
              "ckpt_name": "Illustrious-XL-v2.0.safetensors"
            },
            "class_type": "CheckpointLoaderSimple",
            "_meta": {
              "title": "Load Checkpoint - Illustrious XL v2.0"
            }
          },
          "2": {
            "inputs": {
              "width": 128,
              "height": 128,
              "batch_size": 1
            },
            "class_type": "EmptyLatentImage",
            "_meta": {
              "title": "Empty Latent Image"
            }
          },
          "3": {
            "inputs": {
              "text": "test",
              "clip": ["1", 1]
            },
            "class_type": "CLIPTextEncode",
            "_meta": {
              "title": "CLIP Text Encode (Positive)"
            }
          },
          "4": {
            "inputs": {
              "text": "",
              "clip": ["1", 1]
            },
            "class_type": "CLIPTextEncode",
            "_meta": {
              "title": "CLIP Text Encode (Negative)"
            }
          },
          "5": {
            "inputs": {
              "seed": 42,
              "steps": 4,
              "cfg": 7.0,
              "sampler_name": "euler",
              "scheduler": "normal",
              "denoise": 1.0,
              "model": ["1", 0],
              "positive": ["3", 0],
              "negative": ["4", 0],
              "latent_image": ["2", 0]
            },
            "class_type": "KSampler",
            "_meta": {
              "title": "KSampler"
            }
          },
          "6": {
            "inputs": {
              "samples": ["5", 0],
              "vae": ["1", 2]
            },
            "class_type": "VAEDecode",
            "_meta": {
              "title": "VAE Decode"
            }
          },
          "7": {
            "inputs": {
              "filename_prefix": "illustrious_xl",
              "images": ["6", 0]
            },
            "class_type": "SaveImage",
            "_meta": {
              "title": "Save Image"
            }
          }
        }
      },
      "timeout": 600000
    }
  ],
  "config": {
    "gpuTypeId": "NVIDIA GeForce RTX 4090",
    "gpuCount": 1,
    "env": [
      {
        "key": "REFRESH_WORKER",
        "value": "false"
      }
    ],
    "allowedCudaVersions": ["12.7", "12.6"]
  }
}
